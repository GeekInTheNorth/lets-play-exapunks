LINK 800

MARK REPLICATE_1
COPY 1 T
TEST X = T
TJMP REPLICATE_2
COPY 1 T
REPL NAVIGATE

MARK REPLICATE_2
COPY 3 T
TEST X = T
TJMP REPLICATE_3
COPY 3 T
REPL NAVIGATE

MARK REPLICATE_3
COPY -1 T
TEST X = T
TJMP REPLICATE_4
COPY -1 T
REPL NAVIGATE

MARK REPLICATE_4
COPY -3 T
TEST X = T
TJMP READER
COPY -3 T
REPL NAVIGATE

JUMP READER

MARK NAVIGATE
LINK T
MULI T -1 X
JUMP REPLICATE_1

MARK READER
COPY #NERV X
COPY M T
MARK READER_WAIT
SUBI T 1 T
TJMP READER_WAIT
HOST M
COPY #NERV M